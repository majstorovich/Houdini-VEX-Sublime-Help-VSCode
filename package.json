{
  "name": "houdinivexsublimehelp",
  "displayName": "Houdini VEX Sublime Help",
  "description": "Houdini VEX Sublime Help",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:houdinivexsublimehelp.HoudiniVEXWebviewHelp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "houdinivexsublimehelp.HoudiniVEXWebviewHelp",
        "title": "Houdini VEX Sublime Help"
      }
    ],
    "configuration": [
			{
				"title": "Houdini VEX Sublime Help",
				"properties": {
					"houdinivexsublimehelp.refreshRate": {
						"type": "number",
						"default": 500,
						"description": "Houdini VEX Webview refresh/update rate. Unit in milliseconds.\n\n WARNING: Possible performance issues! \nNot sure if VSCode even refreshes faster than 60Hz. \n\n Maybe don\u2019t go below monitor refresh rate? Testing needed. \n\n 1000 \\ 60Hz = 16.666 \n 1000 \\ 75Hz = 13.333 \n 1000 \\ 120Hz = 8.333 \n 1000 \\ 144Hz = 6.944"
					}
				}
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
